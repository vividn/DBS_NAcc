load_file("nrngui.hoc")
load_file("NAccCell.hoc")

objref cell
cell = new NAccCell(14)
access cell.soma

mu = 1

forsec cell.allSecs{
	if (ismembrane("caL")) {
		for (x,0) {setpointer mu_caL(x), mu}
	}
	if (ismembrane("caL13")) {
		for (x,0) {setpointer mu_caL13(x), mu}
	}
	if (ismembrane("kir")) {
		for (x,0) {setpointer mu_kir(x), mu}
	}
}

objref ic
ic = new IClamp(0.5)

ic.dur = 100
ic.del = 20
ic.amp = 0.5

tstop = 150